{
  "ApplicationName": "elastic-search-with-serilog",
  "ElasticSearch": {
    "Uri": "http://localhost:9200"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "SeqUri": "http://localhost:5341"
  },
  "AllowedHosts": "*"
}
